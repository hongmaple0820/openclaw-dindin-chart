# chat-hub 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# 日志级别 (ERROR | WARN | INFO | DEBUG)
LOG_LEVEL=INFO

# 服务端口
PORT=3000

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# 数据库路径（可选，默认 ~/.openclaw/chat-data/messages.db）
DB_PATH=

# 机器人名称
BOT_NAME=小琳

# 钉钉 Webhook（可选）
DINGTALK_WEBHOOK=
DINGTALK_SECRET=

# OpenClaw 网关（可选）
OPENCLAW_GATEWAY_URL=
OPENCLAW_GATEWAY_TOKEN=
